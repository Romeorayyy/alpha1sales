bleErrorCount":0,"fixableWarningCount":0,"source":"129","usedDeprecatedRules":"99"},{"filePath":"130","messages":"131","suppressedMessages":"132","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/randyyono/Desktop/shopping-cart-react-main/src/index.js",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/App.js",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/contact.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/context/shop-context.jsx",["133"],[],"import { createContext, useEffect, useState } from \"react\";\nimport { PRODUCTS } from \"../products\";\n\nexport const ShopContext = createContext(null);\n\nconst getDefaultCart = () => {\n  let cart = {};\n  for (let i = 1; i < PRODUCTS.length + 1; i++) {\n    cart[i] = 0;\n  }\n  return cart;\n};\n\nexport const ShopContextProvider = (props) => {\n  const [cartItems, setCartItems] = useState(getDefaultCart());\n\n  const getTotalCartAmount = () => {\n    let totalAmount = 0;\n    for (const item in cartItems) {\n      if (cartItems[item] > 0) {\n        let itemInfo = PRODUCTS.find((product) => product.id === Number(item));\n        totalAmount += cartItems[item] * itemInfo.price;\n      }\n    }\n    return totalAmount;\n  };\n\n  const addToCart = (itemId) => {\n    setCartItems((prev) => ({ ...prev, [itemId]: prev[itemId] + 1 }));\n  };\n\n  const removeFromCart = (itemId) => {\n    setCartItems((prev) => ({ ...prev, [itemId]: prev[itemId] - 1 }));\n  };\n\n  const updateCartItemCount = (newAmount, itemId) => {\n    setCartItems((prev) => ({ ...prev, [itemId]: newAmount }));\n  };\n\n  const checkout = () => {\n    setCartItems(getDefaultCart());\n  };\n\n  const contextValue = {\n    cartItems,\n    addToCart,\n    updateCartItemCount,\n    removeFromCart,\n    getTotalCartAmount,\n    checkout,\n  };\n\n  return (\n    <ShopContext.Provider value={contextValue}>\n      {props.children}\n    </ShopContext.Provider>\n  );\n};\n","/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/cart/cart.jsx",["134"],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/components/navbar.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/shop/shop.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/products.js",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/cart/cart-item.jsx",["135"],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/shop/product.jsx",["136"],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/product/ProductPage.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/checkoutform/CheckoutForm.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/index.js",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/App.js",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/components/navbar.jsx",[],[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/contact.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/shop/shop.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/cart/cart.jsx",["137"],[],"import React, { useContext } from \"react\";\nimport { ShopContext } from \"../../context/shop-context\";\nimport { CartItem } from \"../cart/cart-item\";\nimport { PRODUCTS } from \"../../products\";\nimport CheckoutForm from \"../checkoutform/CheckoutForm\"; // import the CheckoutForm component\nimport \"./cart.css\";\n\nconst Cart = () => {\n  const { cartItems, getTotalCartAmount, removeFromCart } = useContext(ShopContext);\n\n  if (getTotalCartAmount() === 0) {\n    return (\n      <div className=\"cart-empty\">\n        <h1>Your cart is empty</h1>\n        <button onClick={() => window.location.href = '/'}>Continue shopping</button>\n      </div>\n    );\n  }\n\n  return (\n    <div className=\"cart\">\n      <div className=\"cart-items\">\n        {Object.entries(cartItems).map(([itemId, quantity]) => {\n          if (quantity === 0) {\n            return null;\n          }\n\n          const product = PRODUCTS.find((product) => product.id === Number(itemId));\n          return <CartItem key={itemId} data={{ ...product, quantity }} />;\n        })}\n      </div>\n      <div className=\"cart-total\">\n        <h2>Total: ${getTotalCartAmount().toFixed(2)}</h2>\n        <CheckoutForm />\n      </div>\n    </div>\n  );\n};\n\nexport default Cart;\n","/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/product/ProductPage.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/checkoutform/CheckoutForm.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/context/shop-context.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/products.js",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/shop/product.jsx",["138"],[],"import React, { useContext } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { ShopContext } from \"../../context/shop-context\";\n\nexport const Product = (props) => {\n  const { id, productName, price, productImage } = props.data;\n  const { addToCart, cartItems } = useContext(ShopContext);\n\n  const cartItemCount = cartItems[id];\n\n  return (\n    <div className=\"product\">\n      <Link to={`/product/${id}`}>\n        <img src={productImage} />\n        <div className=\"description\">\n          <p>\n            <b>{productName}</b>\n          </p>\n          <p> ${price}</p>\n        </div>\n      </Link>\n      <button className=\"addToCartBttn\" onClick={() => addToCart(id)}>\n        Add To Cart {cartItemCount > 0 && <> ({cartItemCount})</>}\n      </button>\n    </div>\n  );\n};\n","/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/cart/cart-item.jsx",["139"],[],"import React, { useContext } from \"react\";\nimport { ShopContext } from \"../../context/shop-context\";\n\nexport const CartItem = (props) => {\n  const { id, productName, price, productImage } = props.data;\n  const { cartItems, addToCart, removeFromCart, updateCartItemCount } =\n    useContext(ShopContext);\n\n  return (\n    <div className=\"cartItem\">\n      <img src={productImage} />\n      <div className=\"description\">\n        <p>\n          <b>{productName}</b>\n        </p>\n        <p> Price: ${price}</p>\n        <div className=\"countHandler\">\n          <button onClick={() => removeFromCart(id)}> - </button>\n          <input\n            value={cartItems[id]}\n            onChange={(e) => updateCartItemCount(Number(e.target.value), id)}\n          />\n          <button onClick={() => addToCart(id)}> + </button>\n        </div>\n      </div>\n    </div>\n  );\n};\n","/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/ThankYou/ThankYou.jsx",[],[],{"ruleId":"140","severity":1,"message":"141","line":1,"column":25,"nodeType":"142","messageId":"143","endLine":1,"endColumn":34},{"ruleId":"144","severity":1,"message":"145","line":50,"column":33,"nodeType":"146","messageId":"147","endLine":50,"endColumn":35},{"ruleId":"148","severity":1,"message":"149","line":11,"column":7,"nodeType":"150","endLine":11,"endColumn":33},{"ruleId":"148","severity":1,"message":"149","line":14,"column":9,"nodeType":"150","endLine":14,"endColumn":35},{"ruleId":"140","severity":1,"message":"151","line":9,"column":42,"nodeType":"142","messageId":"143","endLine":9,"endColumn":56},{"ruleId":"148","severity":1,"message":"149","line":14,"column":9,"nodeType":"150","endLine":14,"endColumn":35},{"ruleId":"148","severity":1,"message":"149","line":11,"column":7,"nodeType":"150","endLine":11,"endColumn":33},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","array-callback-return","Array.prototype.map() expects a value to be returned at the end of arrow function.","