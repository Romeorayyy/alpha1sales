out = () => {\n    setCartItems(getDefaultCart());\n  };\n\n  const contextValue = {\n    cartItems,\n    addToCart,\n    updateCartItemCount,\n    removeFromCart,\n    getTotalCartAmount,\n    checkout,\n  };\n\n  return (\n    <ShopContext.Provider value={contextValue}>\n      {props.children}\n    </ShopContext.Provider>\n  );\n};\n","/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/cart/cart.jsx",["206"],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/components/navbar.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/shop/shop.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/products.js",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/cart/cart-item.jsx",["207"],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/shop/product.jsx",["208"],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/product/ProductPage.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main/src/pages/checkoutform/CheckoutForm.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/index.js",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/App.js",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/components/navbar.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/contact.jsx",[],[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/shop/shop.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/cart/cart.jsx",["209"],[],"import React, { useContext } from \"react\";\nimport { ShopContext } from \"../../context/shop-context\";\nimport { CartItem } from \"../cart/cart-item\";\nimport { PRODUCTS } from \"../../products\";\nimport CheckoutForm from \"../checkoutform/CheckoutForm\"; // import the CheckoutForm component\nimport '../../App.css'\n\nconst Cart = () => {\n  const { cartItems, getTotalCartAmount, removeFromCart } = useContext(ShopContext);\n\n  if (getTotalCartAmount() === 0) {\n    return (\n      <div className=\"cart-empty\">\n        <h1>Your cart is empty</h1>\n        <button onClick={() => window.location.href = '/'}>Continue shopping</button>\n      </div>\n    );\n  }\n\n  return (\n    <div className=\"cart\">\n      <div className=\"cart-items\">\n        {Object.entries(cartItems).map(([itemId, quantity]) => {\n          if (quantity === 0) {\n            return null;\n          }\n\n          const product = PRODUCTS.find((product) => product.id === Number(itemId));\n          return <CartItem key={itemId} data={{ ...product, quantity }} />;\n        })}\n      </div>\n      <div className=\"cart-total\">\n        <h2>Total: ${getTotalCartAmount().toFixed(2)}</h2>\n        <CheckoutForm />\n      </div>\n    </div>\n  );\n};\n\nexport default Cart;\n","/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/product/ProductPage.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/checkoutform/CheckoutForm.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/context/shop-context.jsx",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/products.js",[],[],"/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/shop/product.jsx",["210"],[],"import React, { useContext } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { ShopContext } from \"../../context/shop-context\";\n\nexport const Product = (props) => {\n  const { id, productName, price, productImage } = props.data;\n  const { addToCart, cartItems } = useContext(ShopContext);\n\n  const cartItemCount = cartItems[id];\n\n  return (\n    <div className=\"product\">\n      <Link to={`/product/${id}`}>\n        <img src={productImage} />\n        <div className=\"description\">\n          <p>\n            <b>{productName}</b>\n          </p>\n          <p> ${price}</p>\n        </div>\n      </Link>\n      <button className=\"addToCartBttn\" onClick={() => addToCart(id)}>\n        Add To Cart {cartItemCount > 0 && <> ({cartItemCount})</>}\n      </button>\n    </div>\n  );\n};\n","/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/cart/cart-item.jsx",["211"],[],"import React, { useContext } from \"react\";\nimport { ShopContext } from \"../../context/shop-context\";\n\nexport const CartItem = (props) => {\n  const { id, productName, price, productImage } = props.data;\n  const { cartItems, addToCart, removeFromCart, updateCartItemCount } =\n    useContext(ShopContext);\n\n  return (\n    <div className=\"cartItem\">\n      <img src={productImage} />\n      <div className=\"description\">\n        <p>\n          <b>{productName}</b>\n        </p>\n        <p> Price: ${price}</p>\n        <div className=\"countHandler\">\n          <button onClick={() => removeFromCart(id)}> - </button>\n          <input\n            value={cartItems[id]}\n            onChange={(e) => updateCartItemCount(Number(e.target.value), id)}\n          />\n          <button onClick={() => addToCart(id)}> + </button>\n        </div>\n      </div>\n    </div>\n  );\n};\n","/Users/randyyono/Desktop/shopping-cart-react-main-copy/client/src/pages/ThankYou/ThankYou.jsx",[],[],"/Users/randyyono/Desktop/ragid-shop/client/src/index.js",[],[],"/Users/randyyono/Desktop/ragid-shop/client/src/App.js",[],[],"/Users/randyyono/Desktop/ragid-shop/client/src/components/navbar.jsx",[],[],"/Users/randyyono/Desktop/ragid-shop/client/src/pages/product/ProductPage.jsx",[],[],"/Users/randyyono/Desktop/ragid-shop/client/src/pages/shop/shop.jsx",[],[],"/Users/randyyono/Desktop/ragid-shop/client/src/pages/cart/cart.jsx",["212"],[],"/Users/randyyono/Desktop/ragid-shop/client/src/pages/contact.jsx",[],[],"/Users/randyyono/Desktop/ragid-shop/client/src/pages/checkoutform/CheckoutForm.jsx",[],[],"/Users/randyyono/Desktop/ragid-shop/client/src/context/shop-context.jsx",[],[],"/Users/randyyono/Desk