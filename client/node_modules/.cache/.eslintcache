button onClick={() => removeFromCart(id)}> - </button>\n          <input\n            value={cartItems[id]}\n            onChange={(e) => updateCartItemCount(Number(e.target.value), id)}\n          />\n          <button onClick={() => addToCart(id)}> + </button>\n        </div>\n      </div>\n    </div>\n  );\n};\n",{"ruleId":"135","severity":1,"message":"136","line":1,"column":25,"nodeType":"137","messageId":"138","endLine":1,"endColumn":34},{"ruleId":"139","severity":1,"message":"140","line":50,"column":33,"nodeType":"141","messageId":"142","endLine":50,"endColumn":35},{"ruleId":"143","severity":1,"message":"144","line":11,"column":7,"nodeType":"145","endLine":11,"endColumn":33},{"ruleId":"143","severity":1,"message":"144","line":14,"column":9,"nodeType":"145","endLine":14,"endColumn":35},{"ruleId":"135","severity":1,"message":"146","line":9,"column":42,"nodeType":"137","messageId":"138","endLine":9,"endColumn":56},{"ruleId":"143","severity":1,"message":"144","line":14,"column":9,"nodeType":"145","endLine":14,"endColumn":35},{"ruleId":"143","severity":1,"message":"144","line":11,"column":7,"nodeType":"145","endLine":11,"endColumn":33},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","array-callback-return","Array.prototype.map() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","'removeFromCart' is assigned a value but never used."]